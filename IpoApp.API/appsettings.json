{
    "ConnectionStrings": {
        "DefaultConnection": "Host=localhost;Database=Ipodb;Username=postgres;Password=password;Include Error Detail=True"
       // "DefaultConnection": "Host=host.docker.internal;Port=5432;Database=Ipodb;Username=postgres;Password=password;Include Error Detail=True",
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "JwtSettings": {
        "Secret": "your-256-bit-secret-key-at-least-32-characters",
        "Issuer": "your-issuer",
        "Audience": "your-audience",
        "ExpiryInMinutes": 60
    },
    "SwaggerSettings": {
        "EnableAuthorization": true
    },
    "AllowedHosts": "*"
}